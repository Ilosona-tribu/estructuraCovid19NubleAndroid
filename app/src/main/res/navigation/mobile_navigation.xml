<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_test">

    <fragment
        android:id="@+id/navigation_test"
        android:name="com.example.estructuracovid19nuble.ui.test.TestFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_test" />

    <fragment
        android:id="@+id/navigation_advice"
        android:name="com.example.estructuracovid19nuble.ui.advice.AdviceFragment"
        android:label="@string/title_dashboard"
