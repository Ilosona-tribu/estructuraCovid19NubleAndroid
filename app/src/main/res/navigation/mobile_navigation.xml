    <fragment
        android:id="@+id/navigation_news"
        android:name="com.example.estructuracovid19nuble.ui.news.NewsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.estructuracovid19nuble.ui.map.MapFragment"
        android:label="@string/title_notifications"
